{
    "wrapping": {
        "methodChain": {
            "defaultWrap": "onePerLine",
            "defaultLocation": "afterLast",
            "rules": []
        },
        "callParameter": {
            "rules": [
                {
                    "conditions": [
                        {
                            "cond": "totalItemLength >= n",
                            "value": 70
                        }
                    ],
                    "type": "onePerLine"
                }
            ]
        },
        "maxLineLength": 110
    },
    "emptyLines": {
        "afterLeftCurly": "keep",
        "afterPackage": 1,
        "afterReturn": "remove",
        "classEmptyLines": {
            "beginType": 1
        }
    },
    "whitespace": {
        "arrowFunctionsPolicy": "around",
        "functionTypeHaxe4Policy": "around",
        "functionTypeHaxe3Policy": "around"
    },
    "lineEnds": {
        "leftCurly": "after",
        "rightCurly": "after",
        "emptyCurly": "noBreak"
    }
}